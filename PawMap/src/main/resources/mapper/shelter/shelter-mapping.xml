<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.pawmap.mapper.ShelterMapper">
  	<resultMap type="com.pawmap.VO.ShelterVO" id="shelterList">
  		<id property="shelterSeq" column="shelter_seq"/>
  		<result property="comNum" column="com_num"/>
  		<result property="shelterName" column="shelter_name"/>
		<result property="shleterAddress" column="shelter_address"/>
  		<result property="shelterTel" column="shelter_tel"/>
  		<result property="content" column="content"/>
  	</resultMap> 

  	<!-- (관리자페이지)일반 유저 리스트 -->
  	<select id="getShelterList" resultMap="shelterList">
  		SELECT * FROM pawmap.pet_shelter  <!-- user_seq, user_type, user_id, user_email, user_name, user_nickname, user_tel_num, address, com_num  -->
  				ORDER BY shelter_seq
  	</select>
  </mapper>	